{
  "name": "cssnano",
  "private": true,
  "scripts": {
    "docs": "cd docs && npm run build && cd .. && gh-pages -d docs/dist",
    "fixlint": "eslint --fix --ignore-path .gitignore packages/**/src util",
    "report": "nyc report --reporter=html",
    "pretest": "eslint --ignore-path .gitignore packages/**/src util",
    "test": "nyc ava"
  },
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.0.0",
    "ava": "^0.18.1",
    "babel-cli": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.0.0",
    "babel-register": "^6.0.0",
    "browserslist": "^1.0.0",
    "coveralls": "^2.0.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.0.0",
    "eslint-config-cssnano": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.0",
    "gh-pages": "^0.11.0",
    "glob": "^7.0.0",
    "lerna": "2.0.0-beta.37",
    "nyc": "^10.0.0",
    "postcss": "^5.0.0",
    "write-file": "^1.0.0"
  },
  "ava": {
    "files": [
      "packages/**/src/__tests__/*.js"
    ],
    "concurrency": 5,
    "require": "babel-register"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "**/__tests__",
      "**/dist",
      "util"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "extends": "cssnano"
  },
  "browserslist": {
    "chrome58": [
      "Chrome 58"
    ],
    "edge15": [
      "Edge 15"
    ],
    "ie6": [
      "IE 6"
    ],
    "ie7": [
      "IE 7"
    ],
    "ie8": [
      "IE 8"
    ],
    "ie9": [
      "IE 9"
    ],
    "opera9": [
      "Opera 9"
    ],
    "firefox2": [
      "Firefox 2"
    ]
  }
}
