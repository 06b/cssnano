    "postcss-normalize-charset": "^1.0.0",
    "postcss-normalize-display-values": "0.0.0",
    "postcss-normalize-repeat-style": "0.0.0",
    "postcss-normalize-positions": "0.0.0",
    "postcss-normalize-string": "0.0.0",
    "postcss-normalize-timing-functions": "0.0.0",
    "postcss-normalize-unicode": "0.0.0",
    "postcss-normalize-url": "^3.0.0",
    "postcss-normalize-whitespace": "0.0.0",
    "postcss-ordered-values": "^2.0.0",
    "postcss-reduce-idents": "^2.0.0",
    "postcss-reduce-initial": "^1.0.0",
    "postcss-reduce-transforms": "^1.0.0",
    "postcss-svgo": "^2.0.0",
