    "babel-loader": "^6.0.0",
    "json-loader": "^0.5.4",
    "ncp": "^2.0.0",
    "postcss-font-magician": "^1.0.0",
    "webpack": "^1.0.0",
    "webpack-bundle-size-analyzer": "^2.0.0"
  },
  "homepage": "https://github.com/ben-eb/cssnano",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
